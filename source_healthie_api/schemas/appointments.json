{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "id": {
            "type": ["null", "string"]
        },
        "date": {
            "type": ["null", "string"],
            "format": "date-time"
        },
        "contact_type": {
            "type": ["null", "string"]
        },
        "length": {
            "type": ["null", "integer"]
        },
        "location": {
            "type": ["null", "string"]
        },
        "provider": {
            "type": "object",
            "properties": {
                "id": {
                    "type": ["null", "string"]
                },
                "full_name": {
                    "type": ["null", "string"]
                }
            }
        },
        "appointment_type": {
            "type": "object",
            "properties": {
                "name": {
                    "type": ["null", "string"]
                },
                "id": {
                    "type": ["null", "string"]
                }
            }
        },
        "attendees": {
            "type": ["null", "array"],
            "items": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": ["null", "string"]
                    },
                    "full_name": {
                        "type": ["null", "string"]
                    },
                    "first_name": {
                        "type": ["null", "string"]
                    },
                    "avatar_url": {
                        "type": ["null", "string"]
                    },
                    "phone_number": {
                        "type": ["null", "string"]
                    }
                }
            }
        }
    }
}